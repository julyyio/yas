local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Adopt me!", "Sentinel")


local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Trade Scam")


Section:NewButton("TradeScam", "The new trade scam script", function()
wait (1)
game.StarterGui:SetCore("SendNotification", {Title = "Tradescam", Text = "please wait!", Icon = "rbxassetid://505845268", Duration = 5, Button1 = "Okay"})
    print("Please wait until script is loaded")
end)

local Tab = Window:NewTab("Made By Finessa")
